# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  dykodac_config = debug

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := dykodac

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

dykodac:
ifneq (,$(dykodac_config))
	@echo "==== Building dykodac ($(dykodac_config)) ===="
	@${MAKE} --no-print-directory -C . -f dykodac.make config=$(dykodac_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f dykodac.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   dykodac"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"